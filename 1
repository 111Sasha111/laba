#include <iostream>
#include <cstdlib>

using namespace std;
int main(){
    int m, n;
    cout<<"Enter the number of rows and columns: ";
    cin>>m>>n;

    int **matrix=(int**)malloc(m*sizeof(int*));
    for(int i=0; i<m; i++)
        matrix[i]=(int*)malloc(n*sizeof(int));

    cout<<"Enter the elements of matrix: "<<endl;
    for(int i=0; i<m; i++){
        for(int j=0; j<n; j++){
            cin>>matrix[i][j];
        }
    }

    cout<<"Matrix is: "<<endl;
    for(int i=0; i<m; i++){
        for(int j=0; j<n; j++){
            cout<<matrix[i][j]<<" ";
        }
        cout<<endl;
    }

    for(int i=0; i<m; i++){
        free(matrix[i]);
    }
    free(matrix);
    return 0;
}
